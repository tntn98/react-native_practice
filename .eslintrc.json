{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "prettier"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {"endOfLine" : "auto"}],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", "jsx"]
      }
    ]
  }
}
                        